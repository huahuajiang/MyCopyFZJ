                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <StackPanel Orientation="Horizontal">
                            <Image x:Name="BtnBg" Source="/BaseWindow;component/UI_Images/UC_CertificateKeyBoard/btn_a_1.png"></Image>
