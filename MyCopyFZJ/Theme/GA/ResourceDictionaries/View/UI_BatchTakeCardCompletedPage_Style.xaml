    <Style x:Key="UI_BatchTakeCardCompletedPage_btn_main" TargetType="{x:Type Button}" BasedOn="{ StaticResource ResourceKey=btn_main}">
        <Setter Property="Canvas.Left" Value="964"></Setter>
