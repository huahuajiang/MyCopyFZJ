<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
      <Style x:Key="UI_EmergencyOutCard_wait_gif" TargetType="{x:Type UserControl}">
        <Setter Property="Canvas.Left" Value="590"></Setter>
        <Setter Property="Canvas.Top" Value="384"></Setter>
        <Setter Property="Height" Value="204"></Setter>
        <Setter Property="Width" Value="206"></Setter>
    </Style>

    <Style x:Key="UI_EmergencyOutCard_text_wait_img" TargetType="{x:Type Image}">
        <Setter Property="Canvas.Left" Value="205"></Setter>
        <Setter Property="Canvas.Top" Value="720"></Setter>
        <Setter Property="Source" Value="/MyCopyFZJ;component/Theme/GA/UI_Images/UI_EmergencyOutCard/bg_text_wait_count.png"/>
    </Style>

    <Style x:Key="UI_EmergencyOutCard_Waitcount_label" TargetType="{x:Type Label}">
        <Setter Property="Canvas.Left" Value="400"></Setter>
        <Setter Property="Canvas.Top" Value="725"></Setter>
        <Setter Property="Foreground" Value="White"></Setter>
        <Setter Property="FontSize" Value="26"></Setter>
        <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
        <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
    </Style>

    <Style x:Key="UI_EmergencyOutCard_takeCount_img" TargetType="{x:Type Image}">
        <Setter Property="Canvas.Left" Value="580"></Setter>
        <Setter Property="Canvas.Top" Value="720"></Setter>
        <Setter Property="Source" Value="/MyCopyFZJ;component/Theme/GA/UI_Images/UI_EmergencyOutCard/bg_text_taked_count.png"/>
    </Style>

    <Style x:Key="UI_EmergencyOutCard_takeCount_label" TargetType="{x:Type Label}">
        <Setter Property="Canvas.Left" Value="705"></Setter>
        <Setter Property="Canvas.Top" Value="725"></Setter>
        <Setter Property="Foreground" Value="White"></Setter>
        <Setter Property="FontSize" Value="26"></Setter>
        <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
        <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
    </Style>

    <Style x:Key="UI_EmergencyOutCard_failCount_img" TargetType="{x:Type Image}">
        <Setter Property="Canvas.Left" Value="925"></Setter>
        <Setter Property="Canvas.Top" Value="720"></Setter>
        <Setter Property="Source" Value="/MyCopyFZJ;component/Theme/GA/UI_Images/UI_EmergencyOutCard/bg_text_failed_count.png"/>
    </Style>

    <Style x:Key="UI_EmergencyOutCard_failCount_label" TargetType="{x:Type Label}">
