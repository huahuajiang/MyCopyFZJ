        <Setter Property="FontSize" Value="26"></Setter>
        <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
        <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
    </Style>

    <Style x:Key="UI_EmergencyOutCard_labMessage" TargetType="{x:Type Label}">
        <Setter Property="Canvas.Left" Value="410"></Setter>
        <Setter Property="Canvas.Top" Value="850"></Setter>
        <Setter Property="Height" Value="69"></Setter>
        <Setter Property="Width" Value="519"></Setter>
        <Setter Property="FontSize" Value="35"></Setter>
        <Setter Property="Foreground" Value="Red"></Setter>
        <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
        <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
    </Style>

    <Style x:Key="UI_EmergencyOutCard_gettingCard_img" TargetType="{x:Type Image}">
        <Setter Property="Canvas.Left" Value="575"></Setter>
        <Setter Property="Canvas.Top" Value="596"></Setter>
        <Setter Property="Source" Value="/MyCopyFZJ;component/Theme/GA/UI_Images/UI_EmergencyOutCard/word_gettingcard.png"/>
    </Style>

    <Style x:Key="UI_EmergencyOutCard_btn_main" TargetType="{x:Type Button}" BasedOn="{StaticResource ResourceKey=btn_back}">
        <Setter Property="Canvas.Left" Value="964"></Setter>
        <Setter Property="Canvas.Top" Value="840"></Setter>
    </Style>

    <Style x:Key="UI_EmergencyOutCard_back_img" TargetType="{x:Type Image}">
        <Setter Property="Canvas.Left" Value="310"></Setter>
        <Setter Property="Canvas.Top" Value="155"></Setter>
        <Setter Property="Visibility" Value="Collapsed"></Setter>
    </Style>

</ResourceDictionary>